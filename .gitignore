# .gitignore for pi-extensions
# Node modules
remote-control/node_modules/

# Pi agent cache and sockets
remote-control/.pi/

# Build artifacts
remote-control/*.js
remote-control/*.map
remote-control/*.bin
remote-control/*.swp
remote-control/*.tmp
remote-control/demo/*.js

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Build output
dist/
build/

# Dependency directories
/.pnp/
.pnp.js

# Misc
.DS_Store
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# IDE/editor directories
.idea/
.vscode/
*.sublime-workspace
*.sublime-project

# pi agent cache
.pi/

# Test coverage
coverage/

# Temporary files
~*
*.swp
*.tmp

# Ignore extension binaries
*.bin

